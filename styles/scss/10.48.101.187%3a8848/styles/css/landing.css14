.community-landing {
  background: #333;
  background-image: -webkit-gradient(linear, left top, left bottom, from(rgba(0, 0, 55, 0.25)), to(rgba(0, 0, 55, 0.65))), url(../../images/working.jpg);
  background-image: -o-linear-gradient(top, rgba(0, 0, 55, 0.25) 0%, rgba(0, 0, 55, 0.65) 100%), url(../../images/working.jpg);
  background-image: linear-gradient(180deg, rgba(0, 0, 55, 0.25) 0%, rgba(0, 0, 55, 0.65) 100%), url(../../images/working.jpg);
  background-size: auto 100%;
  background-position: center top;
  background-repeat: no-repeat;
}
.community-landing h1, .community-landing h2 {
  margin-top: calc(var(--baseline)*2);
}
.community-landing h1, .community-landing h2, .community-landing h4 {
  color: #fff;
  margin-left: 1rem;
  text-shadow: 0 0 5px rgba(0, 0, 0, 0.5), 0 0 10px rgba(0, 0, 0, 0.5);
}
.community-landing .community {
  padding: calc(var(--baseline)*2) 0;
}
.community-landing .community img {
  width: 94%;
}

/* ===  FOOTER === */
footer.mainfooter {
  background: #3D383E;
  padding: calc(var(--baseline)) 0 calc(var(--baseline)) 0;
  font-size: 0.75rem;
  color: #ccc;
}
footer.mainfooter h4 {
  color: #ccc;
}
footer.mainfooter ul li a {
  color: #ddd;
}

/* While our default is to  try to scale everything relative to the  HTML{} there are elements 
that need device level targeting. This is where we do it. */
.showmobile, .mobilenav {
  display: none;
}

@media (max-width: 60em) {
  /* tablet adjustments */
  .bookcard .bookframe {
    height: 24vw;
  }

  .container {
    width: 98%;
    margin: 0 1%;
  }
}
@media (max-width: 48em) {
  /* phone adjustments */
  [class~=grid] > [class*=col-12], [class*=grid-] > [class*=col-12], [class*=grid_] > [class*=col-12] {
    -ms-flex-preferred-size: 98%;
    flex-basis: 98%;
  }

  #reeedr .ch-head {
    padding: calc(var(--baseline)*1) 0 0 0;
  }

  .landing-parent .laptop {
    margin: calc(var(--baseline)/2) calc(var(--baseline)*1) calc(var(--baseline)*1) calc(var(--baseline)*1);
  }

  .landing-parent {
    background-size: auto 100%;
  }

  .showmobile, .mobilenav {
    display: block;
  }

  .search-big {
    margin: 0 0 calc(var(--baseline) / 2);
  }
  .search-big .grid-noGutter-center {
    padding: calc(var(--baseline) * 1.5) 0 0;
    margin: 0;
  }
  .search-big input[type*=text] {
    font-size: calc(1rem * var(--typescale) * var(--typescale));
    height: calc(var(--baseline) * 2.5);
    border: 2px solid #ddd;
    border-radius: calc(var(--baseline) * 2) 0 0 calc(var(--baseline) * 2);
  }
  .search-big .insetbutton {
    padding: 1rem;
    height: calc(var(--baseline) * 2.75);
    width: calc(var(--baseline) * 2.75);
  }

  [class*=sm-only] {
    display: none;
  }

  .result-book {
    background: #fff;
    text-align: center;
  }
  .result-book .col-12_sm-2 {
    text-align: left;
  }
  .result-book .thumbframe {
    height: calc(var(--baseline) * 3.5);
    overflow: hidden;
  }
  .result-book img {
    height: 60%;
    width: auto;
    margin-top: calc(var(--baseline)/2);
  }
  .result-book .go {
    line-height: calc(var(--baseline) * 2);
    height: calc(var(--baseline) * 2);
    font-size: calc(1rem * var(--typescale) * var(--typescale));
  }

  .result-text blockquote {
    background: #F8F8F8;
    margin: 0 0 0 0;
    border-radius: 4px;
    max-height: calc(var(--baseline) * 3.5);
    overflow: scroll;
    padding: calc(var(--baseline)/4) 1rem;
  }
  .result-text blockquote p {
    margin: 0;
  }

  .result-book .go span:before {
    width: calc(var(--baseline) / 1);
    height: calc(var(--baseline) / 1);
    top: 0;
    left: -2rem;
  }

  .bookcard .bookframe {
    height: 47vw;
  }
}
/*  <--  end of the media query  */
